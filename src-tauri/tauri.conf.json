{
  "build": {
    "beforeBuildCommand": "pnpm run build",
    "beforeDevCommand": "pnpm run dev",
    "devPath": "http://localhost:1453",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "tauri_cargo_js",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "window": 
      {
        "setPosition": true,
        "setFocus": false,
        "startDragging": true
      },
      "fs": {
        "all": true,
        "scope": ["$APP/*", "$DESKTOP/*"]
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "keyReBoard.hans",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "macOSPrivateApi": true,
    "windows": [
      {
        "fullscreen": false,
        "minWidth": 100,
        "minHeight": 100,
        "resizable": true,
        "transparent": true,
        "alwaysOnTop": true,
        "decorations": false,
        "title": "",
        "hiddenTitle": true,
        "titleBarStyle": "Transparent",
        "width": 800,
        "height": 600
      }
    ]
  }
}
